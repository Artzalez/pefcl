    "@project-error/pe-utils": "^0.2.7",
    "esbuild": "^0.14.11",
    "logform": "^2.3.2",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "winston": "^3.4.0"
