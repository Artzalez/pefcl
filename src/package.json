    "pg-hstore": "^2.3.4",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.17.0",
    "tedious": "^14.3.0",
    "tsyringe": "^4.6.0",
    "winston": "^3.4.0"
  }
}
